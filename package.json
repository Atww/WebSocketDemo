    "express": "4.17.1",
    "express-graphql": "0.12.0",
    "graphql": "15.5.0",
    "graphql-iso-date": "3.6.1",
    "http": "0.0.1-security",
    "nexus": "1.0.0",
    "socket.io": "^3.1.2"
  },
  "devDependencies": {
    "prisma": "^2.17.0",
    "@types/express": "4.17.11",
    "@types/graphql-iso-date": "3.4.0",
    "@types/node": "^14.14.30",
    "@types/socket.io": "^2.1.13",
    "ts-node": "^9.1.1",
    "ts-node-dev": "1.1.1",
    "typescript": "^4.1.5"
